{
  "name": "lans",
  "productName": "Lans",
  "version": "1.1.1",
  "description": "Application to create a list of applications to open at the same time.",
  "author": "Giovani de Oliveira <giovanioliveira@outlook.com.br>",
  "license": "MIT",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "package-macos": "electron-packager ./ --platform=darwin --arch=x64 --out=release-builds --icon=./app/resources/icons/icon.icns --overwrite",
    "package-windows": "electron-packager ./ --platform=win32 --arch=x64 --out=release-builds --icon=./app/resources/icons/icon.ico --overwrite --asar"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xxgicoxx/lans.git"
  },
  "bugs": {
    "url": "https://github.com/xxgicoxx/lans/issues"
  },
  "homepage": "https://github.com/xxgicoxx/lans/",
  "keywords": [
    "lans",
    "open",
    "list",
    "applications"
  ],
  "dependencies": {
    "electron": "^8.0.1",
    "electron-store": "^4.0.0"
  },
  "devDependencies": {
    "electron-packager": "^14.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1"
  }
}
